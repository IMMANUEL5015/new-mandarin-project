  "devDependencies": {
    "coveralls": "^3.1.0",
    "jest": "^26.4.2",
    "node-mocks-http": "^1.9.0",
    "supertest": "^5.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
